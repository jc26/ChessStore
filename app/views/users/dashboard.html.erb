<script src="https://code.highcharts.com/highcharts.js"></script>
<script src="https://code.highcharts.com/modules/exporting.js"></script>

<% if logged_in? && current_user.role?(:admin) %>
  <%= render partial: "admin_dashboard" %>
<% elsif logged_in? && current_user.role?(:manager) %>
  <%= render partial: "manager_dashboard" %>
<% elsif logged_in? && current_user.role?(:shipper) %>
  <%= render partial: "shipper_dashboard" %>
<% elsif logged_in? && current_user.role?(:customer) %>
  <%= render partial: "customer_dashboard" %>
<% end %>
