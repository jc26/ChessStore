$('#new_purchase').remove();
<% if @reorder %>
  $("#new_reorder_link<%= @item.id %>").show();
  $('#reorder-table').replaceWith('<%= j render("partials/items_reorder_list") %>');
<% else %>
  $('#new_purchase_link').show();
  $('#purchase-list').replaceWith('<%= j render("items/purchase_history") %>');
<% end %>
